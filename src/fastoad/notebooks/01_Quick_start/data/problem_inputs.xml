<FASTOAD_model>
  <data>
    <displacements>
      <target units="m" is_input="True">0.02<!--Beam targeted displacement when loads are applied--></target>
    </displacements>
    <forces>
      <F units="N" is_input="True">1000.0<!--Force applied at the tip of the beam--></F>
    </forces>
    <geometry>
      <L units="m" is_input="True">2.0<!--Beam length--></L>
      <l units="m" is_input="True">0.3<!--Beam section width--></l>
    </geometry>
    <material>
      <E units="Pa" is_input="True">70000000000.0<!--Material Young's modulus--></E>
      <density units="kg/m**3" is_input="True">2800.0<!--Material density--></density>
    </material>
  </data>
</FASTOAD_model>
